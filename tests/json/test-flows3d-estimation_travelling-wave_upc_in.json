{
  "general": {
    "storage": {
      "time_stamp": [],
      "dir": "data",
      "dir_tmp": "/tmp",
      "params": {
        "input": {
          "filename": "test-flows3d-estimation_travelling-wave_uac_in.json",
          "dir": "json"
        },
        "output": {
          "filename": "test-flows3d-estimation_travelling-wave_uac_out.json",
          "dir": "json"
        }
      },
      "format": "iomat"
    },
    "parallel": {
      "enabled": true,
      "workers_fraction": 0.8
    }
  },
  "interpolation": {
    "enabled": false
    },
  "flows": {
    "enabled": true,
    "file": {
      "exists": false,
      "keep": true,
      "dir": "",
      "name": "",
      "label": ""
    },
    "method": {
      "data": {
        "mode": "phase"
      },
      "name": "cnem",
      "cnem": {
        "convex_hull": "bi",
        "alpha_smooth": 0.1,
        "max_iterations": 128,
        "burnin": {
          "length": 8
        },
        "initial_conditions": {
          "mode": "random",
          "seed": 42
        },
        "nodal_flows": {
          "enabled": false
        }
      }
    },
    "visualisation": {
      "enabled": true
    },
    "decomposition": {
      "svd": {
        "enabled": false,
        "modes": 4,
        "grid": {
          "type": "structured"
        }
      }
    },
    "streamlines": {
      "enabled": false,
      "grid": {
        "type": "structured"
      }
    }
  },
  "singularity": {
    "enabled": false
  },
  "data": {
    "file": {
      "dir": "data",
      "name": "data-travelling-wave-unstructured-iomat.mat"
    },
    "slice": {
      "enabled": false,
      "id": 0
    },
    "grid": {
      "type": "unstructured"
    },
    "ht": 0.5,
    "hx": [],
    "hy": [],
    "hz": [],
    "units": {
      "space":"mm",
      "time": "ms"
    },
    "boundary": {
      "alpha_radius" : 30
    },
    "mode": "amplitude"
  },
  "visualisation": {
    "quiver": {
      "scale": 1
    }
  }
}