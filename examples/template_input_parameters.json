{
"general": {
    "storage": {
      "time_stamp": [],
      "dir_out": "/path/to/directory/to/store/output/files", 
      "dir_tmp": "/tmp"                                    ,  
      "format": "iomat"
        },
    "params": {
        "input": {
          "file": {
            "dir": "/path/to/input-json",
            "name": "input_params_file.json"
        }
        },
        "output": {
          "file": {
            "dir": "/path/to/output-json",
            "name": "output_params_file.json"
        }
        }
      },
    "parallel": {
      "enabled": true,
      "workers_fraction": 0.8
    }
  },
"data": {
    "file": {
      "dir": "demo-data",
      "name": "rotating_wave_W_c1_d1ms_iomat.mat"
    },
    "slice": {
      "enabled": false,
      "id": 0
    },
    "grid": {
      "type": "unstructured"
    },
    "resolution": {
      "ht": [],
      "hx": [],                                       
      "hy": [],
      "hz": [],
      "units": {
         "space":"mm",                                
          "time": "ms"                               
     }
  },
  "boundary": {
      "alpha_radius" : 30
    },
  "modality": "amplitude"
  },
"interpolation": {
    "enabled": true,
    "file": {
      "exists": false,
      "dir": "",
      "name": "",
      "label": "",
      "keep": true
    },
    "resolution": {
      "ht": [],
      "hx": 3,                                     
      "hy": 3,                                     
      "hz": 3                                    
    },
    "neighbour_method": "",
    "extrapolation_method": "",                                   
    "boundary": {
      "alpha_radius": 30,                          
      "thickness": 2                              
    },
    "visualisation": {
      "enabled": true
    }
  },
"flows": {
  "enabled": true,
    "file": {
      "exists": false,
      "dir": "",
      "name": "",
      "label": "",
      "keep": true
    },
    "method": {
      "data": {
        "modality": "amplitude"
      },
      "name": "hs3d",
      "hs3d": {
        "alpha_smooth": 0.1,
        "max_iterations": 128,
        "initial_conditions": {
          "modality": "random",
          "seed": 42
        },
        "nodal_flows": {
          "enabled": true
        }
      }
    },

    "decomposition": {
      "svd": {
        "enabled": true,
        "modes": 4,
        "grid": {
          "type": "unstructured"
        }
      }
    }
    },

"streamlines": {
      "enabled": false,
      "file": {
          "exists": false,
          "dir": "",
          "name": "",
          "label": "",
          "keep": true
    },
      "grid": {
        "type": "unstructured"
    },
    "visualisation": {
      "enabled": true
    }
  },
  "singularity": {
    "enabled": true,
    "file": {
      "exists": false,
      "dir": "",
      "name": "",
      "label": "",
      "keep": true
    },
    "detection": {
      "enabled": true,
      "modality": "vel",
      "threshold": [
        0,                                  
        0.015625                            
      ]
    },
    "classification": {
      "enabled": true
    },
    "quantification": {
      "enabled": true
    },
    "visualisation": {
      "enabled": true
    }
}
}